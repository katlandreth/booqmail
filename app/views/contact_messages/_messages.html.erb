<table class="table table-striped table-inbox hidden">
  <thead>
    <tr>
      <th>From</th>
      <th>Subject</th>
      <th>Date/Time</th>
    </tr>
    <% @messages.each do |m| %>
    <tr>
      <td><%= m.from %></td>
      <td><%= m.subject %></td>
      <td><%= m.date %></td>
    </tr>
    <% end %>
  </thead>
  <tbody></tbody>
</table>
